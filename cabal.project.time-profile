-- Cabal project configuration file for FUNCTION PROFILING
--
-- Use `cabal` with `--project-file=cabal.project.fun-profile`
-- or run vehicle via `./scripts/vehicle-fun-profile`.

import: cabal.project

profiling: True

package vehicle-syntax
  ghc-options:
    -- Give all bindings automatic SCC annotations.
    -fprof-auto

package vehicle
  ghc-options:
    -- Give all bindings automatic SCC annotations.
    -fprof-auto
