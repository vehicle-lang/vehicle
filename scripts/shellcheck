#!/usr/bin/env cabal
{- cabal:
build-depends:
  , base     >=4       && <5
  , process  >=1.2.0.0 && <2

build-tool-depends: ShellCheck:shellcheck
default-language:   Haskell2010
default-extensions: ImportQualifiedPost
ghc-options:        -Wall
-}
import System.Environment (getArgs)
import System.Process (callProcess)

main :: IO ()
main = do
  args <- getArgs
  callProcess "shellcheck" args
