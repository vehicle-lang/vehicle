-- Indices

eqIndex : Bool
eqIndex = (0 : Index 1) == (1 : Index 2)

neqIndex : Bool
neqIndex = (0 : Index 1) != (1 : Index 2)

leqIndex : Bool
leqIndex = (0 : Index 1) <= (1 : Index 2)

ltIndex : Bool
ltIndex = (0 : Index 1) < (1 : Index 2)

geqIndex : Bool
geqIndex = (0 : Index 1) >= (1 : Index 2)

gtIndex : Bool
gtIndex = (0 : Index 1) > (1 : Index 2)
