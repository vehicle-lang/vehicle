bigAnd : List Bool -> Bool;
bigAnd = fold (\ x -> \ y -> x and y) True

forallIn : (Nat -> Bool) -> List Nat -> Bool;
forallIn f xs = bigAnd (mapList f xs)

vectorToList : Vector Nat 1 -> List Nat;
vectorToList = (\ {A} -> \ {B} -> \ f -> dfold (\ {m} -> f)) (\ x -> \ xs -> x :: xs) nil

mapList : forallT {A} . forallT {B} . (A -> B) -> List A -> List B;
mapList = \ {A} -> \ {B} -> \ f -> fold (\ x -> \ xs -> f x :: xs) nil

@property;
e1 : Bool;
e1 = let in let in x and y

@property;
e2 : Bool;
e2 = let in y >= 2

e3 : Nat;
e3 = let in y

lamLet : Nat -> Nat;
lamLet = \ (x : Nat) -> let in y

letLam : Nat -> Nat;
letLam = let in y

@property;
forallLet : Bool;
forallLet = forall (x : Nat) . let in y == 1

@property;
letForall : Bool;
letForall = let in forall (x : Nat) . y == x

@property;
forallInLet : Bool;
forallInLet = forallIn (\ x -> let in y == 1) (1 :: nil)

@property;
letForallIn : Bool;
letForallIn = let in forallIn (\ x -> y == x) (1 :: nil)

falsey : Rat -> Bool;
falsey x = x <= 0.5

@property;
correctOutput : Bool;
correctOutput = let in falsey y