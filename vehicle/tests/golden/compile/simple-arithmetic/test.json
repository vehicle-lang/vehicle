[
  {
    "name": "TypeCheck",
    "run": "vehicle check -s spec.vcl",
    "needs": ["spec.vcl"]
  },
  {
    "name": "LossFunction",
    "run": "vehicle compile -s spec.vcl -t DL2Loss -e precedence -o LossFunction.json",
    "needs": ["spec.vcl"],
    "produces": ["LossFunction.json"]
  },
  {
    "name": "Marabou",
    "run": "vehicle compile -s spec.vcl -t MarabouQueries -o Marabou.queries/ --network f:fake.onnx",
    "needs": ["spec.vcl", "fake.onnx"],
    "produces": ["Marabou.queries/*.txt"],
    "ignore": {
      "lines": ".*\"fileHash\".*"
    }
  },
  {
    "name": "Agda",
    "run": "vehicle compile -s spec.vcl -t Agda -o Agda.agda",
    "needs": ["spec.vcl"],
    "produces": ["Agda.agda"]
  }
]
