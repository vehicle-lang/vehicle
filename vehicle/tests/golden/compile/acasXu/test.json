[
  {
    "name": "TypeCheck",
    "run": "vehicle compile -s acasXu.vcl -t TypeCheck",
    "needs": ["acasXu.vcl"]
  },
  {
    "name": "Marabou",
    "run": "vehicle compile -s acasXu.vcl -t Marabou -o acasXu.inputquery/ --network acasXu:NO_FILE",
    "needs": ["acasXu.vcl"],
    "produces": ["acasXu.inputquery/*.txt", "acasXu.inputquery/verification-plan.vcle"]
  },
  {
    "name": "Agda",
    "run": "vehicle compile -s acasXu.vcl -t Agda -o acasXu.agda",
    "needs": ["acasXu.vcl"],
    "produces": ["acasXu.agda"]
  }
]
