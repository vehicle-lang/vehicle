mapList : (Rat -> Rat) -> List Rat -> List Rat;
mapList f _x0 = fold (\ x -> \ xs -> f x :: xs) nil _x0

bigAnd : List Rat -> Rat;
bigAnd _x0 = fold (\ x -> \ y -> x + y) 0.0 _x0

type Tensor A ds = fold (\ d -> \ t -> Vector t d) A ds

@property;
bounded : forallT (f : Tensor Rat (1 :: nil) -> Tensor Rat (1 :: nil)) . Rat;
bounded f = bigAnd (mapList (\ x -> max 0.0 ((max 0.0 (0.0 - x) + (if 0.0 == x then 1.0 else 0.0) + (max 0.0 (x - 1.0) + (if x == 1.0 then 1.0 else 0.0))) * (max 0.0 (0.0 - f [x] ! 0) + (if 0.0 == f [x] ! 0 then 1.0 else 0.0) + (max 0.0 (f [x] ! 0 - 1.0) + (if f [x] ! 0 == 1.0 then 1.0 else 0.0))))) sample[x][])