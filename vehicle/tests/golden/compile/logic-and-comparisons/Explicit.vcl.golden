@property;
test1 : forallT (f : Tensor Rat -> Tensor Rat) . Tensor Bool;
test1 f = exists a . a <= 0.0 or ReduceAnd (f [a + 2.0] == [0.0])

@property;
test2 : forallT (g : Tensor Rat -> Tensor Rat) . Tensor Bool;
test2 g = exists a . exists b . a >= 1.0 and not (b > a) and ReduceAnd (g [a + b, a + 2.0 * b] == [0.0])

@property;
test3 : forallT (g : Tensor Rat -> Tensor Rat) . Tensor Bool;
test3 g = forall a . forall b . not ((a < 0.0 and b != 0.0) or ReduceOr (g [a, b] != [0.0]))