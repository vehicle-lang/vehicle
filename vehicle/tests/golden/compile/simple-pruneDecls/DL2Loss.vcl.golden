@property;
p1 : forallT (f : Tensor Rat -> Tensor Rat) . Tensor Bool;
p1 f = Optimise[min] (+) (\ x -> max 0.0 (1.0 - f x ! 0) + (if 1.0 == f x ! 0 then 1.0 else 0.0))

@property;
p2 : forallT (g : Tensor Rat -> Tensor Rat) . Tensor Bool;
p2 g = Optimise[min] (+) (\ x -> max 0.0 (0.0 - g x ! 0) + (if 0.0 == g x ! 0 then 1.0 else 0.0))