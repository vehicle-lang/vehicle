mapList__Vector_Rat_1__Rat : (Vector Rat 1 -> Rat) -> List (Vector Rat 1) -> List Rat;
mapList__Vector_Rat_1__Rat f = fold (\ x -> \ xs -> f x :: xs) nil

mapList__Vector_Rat_2__Rat : (Vector Rat 2 -> Rat) -> List (Vector Rat 2) -> List Rat;
mapList__Vector_Rat_2__Rat f = fold (\ x -> \ xs -> f x :: xs) nil

bigAnd : List Rat -> Rat;
bigAnd = fold (\ x -> \ y -> x + y) 0.0

add1 : Rat -> Rat;
add1 x = x + 1.0

@property;
p1 : forallT (f : Vector Rat 1 -> Vector Rat 1) . Rat;
p1 f = bigAnd (mapList__Vector_Rat_1__Rat (\ x -> max 0.0 (add1 0.0 - f x ! 0)) sample[x][])

@property;
p2 : forallT (g : Vector Rat 2 -> Vector Rat 1) . Rat;
p2 g = bigAnd (mapList__Vector_Rat_2__Rat (\ x -> max 0.0 (0.0 - g x ! 0)) sample[x][])