[
  {
    "name": "TypeCheck",
    "run": "vehicle check -s spec.vcl",
    "needs": ["spec.vcl"]
  },
  {
    "name": "Marabou",
    "run": "vehicle compile -s spec.vcl -t MarabouQueries -o Marabou.queries --network g:fake.onnx -e p2",
    "needs": ["spec.vcl", "fake.onnx"],
    "produces": ["Marabou.queries/*.txt", "Marabou.queries/.vcl-plan"],
    "ignore":
    {
      "lines": ".*\"fileHash\".*"
    }
  },
  {
    "name": "Agda",
    "run": "vehicle compile -s spec.vcl -t Agda -o Agda.agda --network f:NO_FILE --network g:NO_FILE -e p2",
    "needs": ["spec.vcl"],
    "produces": ["Agda.agda"]
  },
  {
    "name": "LossFunction",
    "run": "vehicle compile -s spec.vcl -t DL2Loss -o LossFunction.json",
    "needs": ["spec.vcl"],
    "produces": ["LossFunction.json"]
  }
]
