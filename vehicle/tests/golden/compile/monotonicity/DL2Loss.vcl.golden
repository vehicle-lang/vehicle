mapList : (Rat -> Rat) -> List Rat -> List Rat;
mapList f _x0 = fold (\ x -> \ xs -> f x :: xs) nil _x0

bigAnd : List Rat -> Rat;
bigAnd _x0 = fold (\ x -> \ y -> x + y) 0.0 _x0

type Tensor A ds = fold (\ d -> \ t -> Vector t d) A ds

@property;
monotonic : forallT (f : Tensor Rat (1 :: nil) -> Tensor Rat (1 :: nil)) . Rat;
monotonic f = bigAnd (mapList (\ x1 -> bigAnd (mapList (\ x2 -> max 0.0 (max 0.0 (x1 - x2) * max 0.0 (f [x1] ! 0 - f [x2] ! 0))) sample[x2][x1])) sample[x1][])