[
  {
    "name": "TypeCheck",
    "run": "vehicle compile -s increasing.vcl -t TypeCheck",
    "needs": ["increasing.vcl"]
  },
  {
    "name": "Marabou",
    "run": "vehicle compile -s increasing.vcl -t Marabou -o increasing.inputquery/ --network f:NO_FILE",
    "needs": ["increasing.vcl"],
    "produces": ["increasing.inputquery/*.txt", "increasing.inputquery/verification-plan.vcle"]
  },
  {
    "name": "Agda",
    "run": "vehicle compile -s increasing.vcl -t Agda -o increasing.agda",
    "needs": ["increasing.vcl"],
    "produces": ["increasing.agda"]
  }
]
