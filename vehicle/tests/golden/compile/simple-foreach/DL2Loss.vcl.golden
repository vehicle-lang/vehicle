@property;
index : (Tensor Rat -> Tensor Rat) -> Tensor Rat;
index f = [max (const[ 0.0 ] nil) (const[ 0.0 ] nil - f [const[ 0.0 ] nil, const[ 0.0 ] nil] ! 0), max (const[ 0.0 ] nil) (const[ 0.0 ] nil - f [const[ 0.0 ] nil, const[ 0.0 ] nil] ! 1)]

@property;
safe : (Tensor Rat -> Tensor Rat) -> Tensor Rat;
safe f = minimise (+) (\ x -> max (const[ 0.0 ] nil) (const[ 0.0 ] nil - f (x + const[ 4.0 ] (2 :: nil)) ! 0))